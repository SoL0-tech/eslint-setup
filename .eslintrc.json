{
    "parser": "typescript-eslint-parser",
    "env": {
        "node": true
    },
    "indent": ["error", "tab"],
    "plugins": [
        "prefer-const",
        "no-const-assign",
        "no-var",
        "no-new-object",
        "object-shorthand",
        "quote-props",
        "no-array-constructor",
        "array-callback-return",
        "prefer-destructuring",
        "quotes",
        "prefer-template",
        "template-curly-spacing",
        "no-eval",
        "no-useless-escape",
        "func-style",
        "wrap-iife",
        "no-loop-func",
        "prefer-rest-params",
        "no-new-func",
        "space-before-function-paren",
        "space-before-blocks",
        "no-param-reassign",
        "prefer-spread",
        "prefer-arrow-callback",
        "arrow-spacing",
        "arrow-parens",
        "arrow-body-style",
        "no-confusing-arrow",
        "no-useless-constructor",
        "no-dupe-class-members",
        "no-duplicate-imports",
        "import/no-mutable-exports",
        "import/prefer-default-export",
        "import/first",
        "import/no-webpack-loader-syntax",
        "no-iterator",
        "no-restricted-syntax",
        "generator-star-spacing",
        "dot-notation",
        "no-restricted-properties",
        "no-undef",
        "one-var",
        "no-multi-assign",
        "no-plusplus",
        "eqeqeq",
        "no-case-declarations",
        "no-nested-ternary",
        "no-unneeded-ternary",
        "no-mixed-operators",
        "nonblock-statement-body-position",
        "brace-style",
        "no-else-return",
        "spaced-comment",
        "space-before-blocks",
        "keyword-spacing",
        "space-infix-ops",
        "eol-last",
        "newline-per-chained-call",
        "no-whitespace-before-property",
        "padded-blocks",
        "space-in-parens",
        "array-bracket-spacing",
        "object-curly-spacing",
        "max-len",
        "comma-style",
        "comma-dangle",
        "semi",
        "no-new-wrappers",
        "radix",
        "id-length",
        "camelcase",
        "new-cap",
        "no-underscore-dangle",
        "no-restricted-globals"
    ]
}